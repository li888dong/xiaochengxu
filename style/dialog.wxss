.dialog{
    position: absolute;
    width: 80%;
    padding: 60rpx 30rpx 20rpx;
    top: 20%;
    left: 50%;
    -webkit-transform: translateX(-50%);
	-moz-transform: translateX(-50%);
	-ms-transform: translateX(-50%);
	-o-transform: translateX(-50%);
	transform: translateX(-50%);
    background: #fff;
    z-index: 1000;
}

.dialog_memb{ 
    display: block; 
    background: #000; 
    position: fixed; 
    width: 100%; 
    height: 100%; 
    top:0; 
    left:0; 
    z-index: 200; 
    opacity: .6; 
}


